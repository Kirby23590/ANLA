  <!-- ==================== Mini SMG T1 ========================== -->

  <ThingDef ParentName="BaseHumanMakeableGun">
    <defName>Gun_MiniSMG_ANLA</defName>
    <label>mini SMG</label>
    <description>A small and compact machine pistol. It has a very short range &amp; lacks strong punch, but fires five bullets in a burst to make up for it's weakness.</description>
    <graphicData>
      <texPath>Weapons/SMGs/MiniSMG</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_SMG</soundInteract>
    <recipeMaker>
      <researchPrerequisite>BlowbackOperation</researchPrerequisite>
    </recipeMaker>
    <statBases>
      <WorkToMake>20000</WorkToMake>
      <Mass>1.2</Mass>
      <AccuracyTouch>0.50</AccuracyTouch>
      <AccuracyShort>0.50</AccuracyShort>
      <AccuracyMedium>0.30</AccuracyMedium>
      <AccuracyLong>0.10</AccuracyLong>
      <RangedWeapon_Cooldown>1.25</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>38</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>ANLABullet_MiniSMG</defaultProjectile>
        <warmupTime>0.8</warmupTime>
        <range>15.9</range>
        <burstShotCount>5</burstShotCount>
        <ticksBetweenBurstShots>7</ticksBetweenBurstShots>
        <soundCast>Shot_MachinePistol</soundCast>
        <soundCastTail>GunTail_Light</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
	    <weaponTags>
			<li>ScavGun_ANLA</li>
			<li>VilagerWeap_ANLA</li>
		</weaponTags>
    <tools>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5.5</power>
        <cooldownTime>1.7</cooldownTime>
      </li>
    </tools>
	<thingCategories Inherit="False">
      <li>WeaponsRanged_SMG_ANLA</li>
    </thingCategories>
  </ThingDef>
